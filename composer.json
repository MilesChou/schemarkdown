{
    "name": "mileschou/schemarkdown",
    "description": "The schema document generator",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "database"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "doctrine/dbal": "^2.6",
        "illuminate/console": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/database": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/events": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/support": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/view": "^5.6 | ^6 | ^7 | ^8",
        "mikey179/vfsstream": "^1.6.7",
        "mileschou/codegener": "^1.0.2"
    },
    "require-dev": {
        "illuminate/container": "^5.6 | ^6 | ^7 | ^8",
        "laravel-bridge/scratch": "^1.3",
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9",
        "psy/psysh": "^0.10.7",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Schemarkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "MilesChou\\Schemarkdown\\SchemarkdownServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
