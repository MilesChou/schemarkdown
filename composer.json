{
    "name": "mileschou/schemarkdown",
    "description": "The schema generate helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "database"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^2.6",
        "illuminate/console": "^8 | ^9 | ^10",
        "illuminate/database": "^8 | ^9 | ^10",
        "illuminate/events": "^8 | ^9 | ^10",
        "illuminate/support": "^8 | ^9 | ^10",
        "illuminate/view": "^8 | ^9 | ^10",
        "mikey179/vfsstream": "^1.6.7",
        "mileschou/codegener": "^1.0.2"
    },
    "require-dev": {
        "illuminate/container": "^8 | ^9 | ^10",
        "laravel-bridge/scratch": "^1.3",
        "phpunit/phpunit": "^9.6",
        "psy/psysh": "^0.11.15",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^6"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Schemarkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "MilesChou\\Schemarkdown\\SchemarkdownServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
