{
    "name": "mileschou/schemarkdown",
    "description": "The schema document generator",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "database"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.6",
        "illuminate/database": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/events": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/support": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/view": "^5.6 | ^6 | ^7 | ^8",
        "mileschou/codegener": "^1.0.2"
    },
    "require-dev": {
        "illuminate/container": "^5.6 | ^6 | ^7 | ^8",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9",
        "psy/psysh": "^0.10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Schemarkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
